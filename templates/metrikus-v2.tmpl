{
  "objects": [
    {
      "key": "{{ $.user_id }}",
      "type": "user",
      "displayName": "{{ $.nickname }}",
      "created_at":"{{ $.created_at }}",
      "properties":{
        "email": "{{ $.email }}",
        "picture": "{{ $.picture }}",
        "user_props": {
          {{$s := separator ", "}}{{ range $k, $v := $.user_metadata }}{{call $s}}"{{ $k }}": "{{ $v }}"{{ end }}
        },
        "app_props": {
          {{$s := separator ", "}}{{ range $k, $v := $.app_metadata }}{{call $s}}"{{ $k }}": "{{ $v }}"{{ end }}
        }
      }
    },
    {{ range $index, $element := $.identities }}
      {{ if $index }},{{ end }}
      {
        "key": "{{$element.user_id}}",
        "type": "identity"
      }
    {{ end }}

    {{ if $.roles }}, {{ end }}

    {{ range $i, $element := $.roles }}
      {{ if $i }},{{ end }}
      {
        "key": "{{$element.name}}",
        "type": "group"
      }
    {{ end }}
  ],
  "relations": [    
    {{ range $i,$element := $.identities }} 
      {{ if $i }},{{ end }}
      {
      "relation": "identifier",
              "subject": {
                  "type": "user",
                  "key": "{{$.user_id}}"
              },
              "object": {
                  "type": "identity",
                  "key": "{{$element.user_id}}"
              }
      }
    {{ end }}
  ]
}